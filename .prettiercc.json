{
	"$schema": "https://json.schemastore.org/prettierrc",
	"printWidth": 120,
	"useTabs": true,
	"tabWidth": 2,
	"semi": true,
	"singleQuote": true,
	"quoteProps": "as-needed",
	"jsxSingleQuote": true,
	"trailingComma": "es5",
	"bracketSpacing": true,
	"bracketSameLine": false,
	"arrowParens": "always",
	"singleAttributePerLine": false
}
